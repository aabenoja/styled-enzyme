  "peerDependencies": {
    "enzyme": "*",
    "styled-components": "*",
    "react": "*",
    "react-test-renderer": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.3",
    "jest-styled-components": "^5.0.1",
    "react": "^16.3.2",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^3.0.7",
    "styled-components": "^3.2.6"
  }
